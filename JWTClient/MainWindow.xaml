<Window x:Class="JWTClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JWTClient"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>
        <WrapPanel Margin="10">
            <Button Content="Login" Width="120" Height="40" Click="Button_Click"></Button>
            <Button x:Name="btnGetList" Content="GetList" Width="120" Height="40" Click="btnGetList_Click" Margin="5,0"></Button>
        </WrapPanel>
        <ListBox x:Name="lb" Height="250" Grid.Row="1" Margin="10">

        </ListBox>
    </Grid>
</Window>
